# Jaguars                   #
#############################

# --- El Jefe ---
:ElJefe a ont:Jaguar ;
    rdfs:label "El Jefe" ;
    ont:scientificName "Panthera onca" ;
    ont:hasGender "Male" ;
    ont:hasDietType :CarnivoreDiet ;
    ont:occursIn :WhetstoneMountains, :SantaRitaMountains, :Sonora ;
    ont:originatesFrom :Sonora ;
    ont:namedBy :FelizardoStudents ;
    ont:monitoredByOrg :UnivOfArizona, :ConservationCATalyst, :NorthernJaguarProject ;
    ont:monitoredByTechnique :CameraTrap, :ScatDetection ;
    ont:hasIdentificationMark "Unique spot patterns" ;
    ont:hasMonitoringStartDate "2011-11-19"^^xsd:date ;
    ont:hasLastSightingDate "2021-11-27"^^xsd:date ;
    ont:hasObservation :ElJefeFirstSighting, :ElJefeSonoraSighting .

:ElJefeFirstSighting a ont:Observation ;
    ont:observedDate "2011-11-19"^^xsd:date ;
    ont:observedBy :DonnieFenn .

:ElJefeSonoraSighting a ont:Observation ;
    ont:observedDate "2021-11-27"^^xsd:date ;
    ont:monitoredByOrg :NorthernJaguarProject .

