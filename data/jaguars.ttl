@prefix ont: <http://example.org/ontology#> .
@prefix : <http://example.org/resource#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

#############################
# Locations & Habitat Areas
#############################

:USA a ont:Country ; rdfs:label "United States of America" .
:Mexico a ont:Country ; rdfs:label "Mexico" .
:Brazil a ont:Country ; rdfs:label "Brazil" .
:Colombia a ont:Country ; rdfs:label "Colombia" .
:Argentina a ont:Country ; rdfs:label "Argentina" .
:Belize a ont:Country ; rdfs:label "Belize" .

:Arizona a ont:State ; ont:locatedInCountry :USA ; rdfs:label "Arizona" .
:Sonora a ont:State ; ont:locatedInCountry :Mexico ; rdfs:label "Sonora" .

:WhetstoneMountains a ont:MountainRange ; rdfs:label "Whetstone Mountains" .
:SantaRitaMountains a ont:MountainRange ; rdfs:label "Santa Rita Mountains" .
:ChiricahuaMountains a ont:MountainRange ; rdfs:label "Chiricahua Mountains" .
:HuachucaMountains a ont:MountainRange ; rdfs:label "Huachuca Mountains" .
:DosCabezasMountains a ont:MountainRange ; rdfs:label "Dos Cabezas Mountains" .

:LlanosRegion a ont:Region ; rdfs:label "Llanos (Colombia)" .
:PantanalRegion a ont:Region ; rdfs:label "Pantanal (Brazil)" .

# Habitat areas used in corridors/reserves/wetlands
:NorthernJaguarReserveArea a ont:HabitatArea ;
  rdfs:label "Northern Jaguar Reserve (Sonora)" ;
  ont:hasAcreage 60300 .

:SkyIslandsBorderlands a ont:HabitatArea ;
  rdfs:label "Arizona–Sonora Sky Islands borderlands" .

:IberaWetlandsArea a ont:HabitatArea ;
  rdfs:label "Iberá Wetlands (Argentina)" .

#############################
# Habitats (and where they are)
#############################

:PantanalWetland a ont:Wetland ; ont:locatedIn :PantanalRegion ; rdfs:label "Pantanal wetland habitat" .
:IberaWetlandHabitat a ont:Wetland ; ont:locatedIn :IberaWetlandsArea ; rdfs:label "Iberá wetland habitat" .
:SkyIslandsForest a ont:Forest ; ont:locatedIn :SkyIslandsBorderlands ; rdfs:label "Sky Islands montane forest" .

#############################
# Monitoring Techniques (instances)
#############################

:techCameraTrap a ont:CameraTrap ; rdfs:label "Camera-trap monitoring" .
:techGPSTrack a ont:GPSTracking ; rdfs:label "GPS/radio tracking" .
:techScatDetect a ont:ScatDetection ; rdfs:label "Scat detection (dog-assisted)" .

#############################
# Organizations
#############################

:USFWS a ont:GovernmentAgency ; rdfs:label "U.S. Fish and Wildlife Service" .
:AZGFD a ont:GovernmentAgency ; rdfs:label "Arizona Game and Fish Department" .
:UArizona a ont:AcademicInstitution ; rdfs:label "University of Arizona" .
:CenterForBiologicalDiversity a ont:NGO ; rdfs:label "Center for Biological Diversity" .
:ConservationCATalyst a ont:NGO ; rdfs:label "Conservation CATalyst" .
:WildlandsNetwork a ont:NGO ; rdfs:label "Wildlands Network" .
:PROFAUNA a ont:NGO ; rdfs:label "Protección de la Fauna Mexicana (PROFAUNA)" .
:NorthernJaguarProject a ont:NGO ; rdfs:label "Northern Jaguar Project" .
:Naturalia a ont:NGO ; rdfs:label "Naturalia AC" .
:WCS a ont:NGO ; rdfs:label "Wildlife Conservation Society" .
:PantheraNGO a ont:NGO ; rdfs:label "Panthera" .
:Oncafari a ont:NGO ; rdfs:label "Onçafari" .
:NEX a ont:NGO ; rdfs:label "NEX No Extinction" .

#############################
# Conservation Efforts & Legal
#############################

:USJaguarRecoveryPlan a ont:RecoveryPlan ;
  rdfs:label "US Jaguar Species Recovery Plan" .

:WesternMX_AZ_Corridor a ont:WildlifeCorridor ;
  rdfs:label "Western Mexico–Arizona Jaguar Corridor" ;
  ont:connectsHabitat :NorthernJaguarReserveArea, :SkyIslandsBorderlands .

:OncafariRewilding a ont:RewildingProgram ;
  rdfs:label "Onçafari Jaguar Rewilding & Release Program" .

:InternationalBorderlandsCoop a ont:InternationalCooperation ;
  rdfs:label "Borderlands Linkages Initiative cooperation" .

:EndangeredSpeciesAct a ont:Act ;
  rdfs:label "Endangered Species Act (United States)" .

# Orgs implementing efforts
:USFWS ont:implementsEffort :USJaguarRecoveryPlan .
:WildlandsNetwork ont:implementsEffort :InternationalBorderlandsCoop .
:PROFAUNA ont:implementsEffort :InternationalBorderlandsCoop .
:NorthernJaguarProject ont:implementsEffort :InternationalBorderlandsCoop .
:Oncafari ont:implementsEffort :OncafariRewilding .

#############################
# Threats
#############################

:PoachingThreat a ont:Poaching ; rdfs:label "Poaching" .
:HumanWildlifeConflictThreat a ont:HumanWildlifeConflict ; rdfs:label "Human–wildlife conflict" .
:BorderBarrierThreat a ont:BorderBarrier ; rdfs:label "Border barrier / permeability" .
:WildfireThreat a ont:Wildfire ; rdfs:label "Wildfire" .
:ClimateChangeThreat a ont:ClimateChange ; rdfs:label "Climate change" .
:IllegalWildlifeTradeThreat a ont:IllegalWildlifeTrade ; rdfs:label "Illegal wildlife trade" .

#############################
# People (observers/naming)
#############################

:DonnieFenn a ont:Person ; rdfs:label "Donnie Fenn" .
:FelizardoValenciaStudents a ont:Person ; rdfs:label "Felizardo Valencia Middle School students" .
:TohonoOodham_PY_Dine_Students a ont:Person ;
  rdfs:label "Tohono O'odham, Pascua Yaqui & Diné schoolchildren" .

#############################
# Populations
#############################

:HuasabasSahuaripaPop a ont:JaguarPopulation ;
  rdfs:label "Huásabas–Sahuaripa jaguar population (Sonora)" .

:HatoLaAuroraPop a ont:JaguarPopulation ;
  rdfs:label "Hato la Aurora jaguar population (Colombia Llanos)" ;
  ont:isDependentOn :HuasabasSahuaripaPop .

