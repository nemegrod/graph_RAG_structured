#############################

# El Jefe
:ElJefe a ont:Jaguar ;
  ont:scientificName "Panthera onca" ;
  ont:hasGender "Male" ;
  ont:hasDietType ont:CarnivoreDiet ;
  ont:occursIn :Arizona, :WhetstoneMountains, :SantaRitaMountains, :Sonora ;
  ont:originatesFrom :Sonora ;
  ont:monitoredByTechnique :techCameraTrap, :techScatDetect ;
  ont:monitoredByOrg :USFWS, :AZGFD, :UArizona, :ConservationCATalyst, :CenterForBiologicalDiversity, :NorthernJaguarProject, :PROFAUNA, :WildlandsNetwork ;
  ont:facesThreat :HumanWildlifeConflictThreat, :BorderBarrierThreat .

# Observations for El Jefe
:Obs_ElJefe_2011 a ont:Observation ;
  rdfs:label "El Jefe first recorded (Whetstone Mtns)" ;
  ont:observedDate "2011-11-19"^^xsd:date ;
  ont:observedBy :DonnieFenn .
:ElJefe ont:hasObservation :Obs_ElJefe_2011 .

:Obs_ElJefe_2021_Sonora a ont:Observation ;
  rdfs:label "El Jefe photographed (central Sonora)" ;
  ont:observedDate "2021-11-27"^^xsd:date .
